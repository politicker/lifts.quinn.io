	"version": "0.2.0",
	"configurations": [
		{
			"name": "Web Server",
			"cwd": "${workspaceFolder}",
			"type": "go",
			"request": "launch",
			"program": "${workspaceFolder}/cmd/main.go",
			"args": [
				"web"
			],
			"envFile": "${workspaceFolder}/.env"
		},
		{
			"name": "Import lifts",
			"cwd": "${workspaceFolder}",
			"type": "go",
			"request": "launch",
			"program": "${workspaceFolder}/cmd/main.go",
			"args": [
				"extract",
				"--file",
				"strong.csv"
			],
			"envFile": "${workspaceFolder}/.env"
		},
	]
}
